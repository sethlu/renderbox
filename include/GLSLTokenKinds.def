
#ifndef TOK
#define TOK(X)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(X,Y) TOK(X)
#endif
#ifndef PPKEYWORD
#define PPKEYWORD(X)
#endif


// Preprocessor keywords

PPKEYWORD(include)


// Language keywords

TOK(unknown)             // Not a token
TOK(eof)                 // End of file


#undef PPKEYWORD
#undef PUNCTUATOR
#undef TOK
