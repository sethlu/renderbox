
#ifndef TOK
#define TOK(X)
#endif
#ifndef PPKEYWORD
#define PPKEYWORD(X)
#endif


// Preprocessor keywords

PPKEYWORD(not_keyword)

PPKEYWORD(include)


// Language keywords

TOK(unknown)            // Not a token
TOK(eod)                // End of directive
TOK(eof)                // End of file

TOK(identifier)         // Identifier

#undef PPKEYWORD
#undef TOK
