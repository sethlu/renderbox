
#ifndef TOK
#define TOK(X)
#endif
#ifndef PPKEYWORD
#define PPKEYWORD(X)
#endif


// Preprocessor keywords

PPKEYWORD(not_keyword)

PPKEYWORD(include)
PPKEYWORD(version)


// Language keywords

TOK(unknown)              // Not a token
TOK(eod)                  // End of directive
TOK(eof)                  // End of file

TOK(identifier)           // Identifier

TOK(string_literal)       // "foo"
TOK(angle_string_literal) // <foo>


#undef PPKEYWORD
#undef TOK
